<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.contratediarista.br.contratediarista.ui.VisualizarVagasJaCandidatouUi">

    <TextView
        android:id="@+id/tv_nenhuma_vaga"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:text="Você não se candidatou a nenhuma vaga"
        android:textAlignment="center" />

    <ListView
        android:id="@+id/lv_vagas_que_se_candidatou"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="8dp"
        android:divider="@color/divider"></ListView>

</FrameLayout>
